{
  "name": "node-express-typegraphql-typeorm",
  "version": "1.1.1",
  "description": "Simple Node.js/Express boilerplate API using TypeGraphQL and TypeORM.",
  "main": "index.js",
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts && yarn lint",
    "serve": "node build/server.js",
    "watch-node": "nodemon build/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "test": "jest --runInBand --watchAll ./src/tests",
    "start-and-test": "env NODE_ENV=test server-test 3002",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts",
    "lint-fix": "tsc --noEmit && eslint . --ext .js,.ts --fix"
  },
  "author": "Martin Trajanovski <martin.trajanovski@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "_moduleAliases": {
    "@src": "build"
  },
  "dependencies": {
    "apollo-server-express": "^2.10.1",
    "axios": "^0.19.2",
    "bcrypt": "3.0.7",
    "body-parser": "^1.19.0",
    "chalk": "2.4.1",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cors": "2.8.3",
    "dotenv": "^8.2.0",
    "envalid": "^4.2.2",
    "express": "^4.17.1",
    "express-jwt": "^5.3.0",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "path": "0.12.7",
    "pg": "^7.18.1",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "start-server-and-test": "^1.10.8",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.22",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/concurrently": "^4.1.0",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.9",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lusca": "^1.6.1",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.8",
    "@types/redis": "^2.8.15",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "node-gyp": "^3.8.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.6.3"
  }
}
